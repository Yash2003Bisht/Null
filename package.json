{
  "name": "null",
  "displayName": "null",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:*",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "inlineCompletions": [
      {
        "language": "*",
        "scheme": "file"
      }
    ],
    "commands": [
      {
        "command": "codeCompletion.setApiKey",
        "title": "Null Code: Set API Key and Provider"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Null Code: Set API Key and Provider",
      "properties": {
        "codeCompletion.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key.",
          "scope": "window"
        },
        "codeCompletion.provider": {
          "type": "string",
          "default": "",
          "enum": [
            "openai",
            "anthropic"
          ],
          "description": "The provider to use for code completion.",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.8",
    "@langchain/core": "^0.3.18",
    "@langchain/openai": "^0.3.13",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "langchain": "^0.3.5",
    "openai": "^4.71.1"
  }
}